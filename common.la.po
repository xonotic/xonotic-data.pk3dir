msgstr ""

#: qcsrc/common/weapons/all.qc:883
#, c-format
msgid "The %s does %s damage per second"
msgstr ""

#: qcsrc/common/weapons/all.qc:884
#, c-format
msgid "The %s shoots %s shots doing %s damage each"
msgstr ""

#: qcsrc/common/weapons/all.qc:885
