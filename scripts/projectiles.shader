arc_projectile_core
{
	surfaceparm nonsolid
	surfaceparm nolightmap
	deformVertexes autosprite
	{
		map textures/projectiles/arc_projectile_core.tga
		blendfunc GL_SRC_ALPHA GL_ONE
		rgbGen lightingDiffuse
	}
}
